{
  "name": "ajv-openapi-compile",
  "version": "0.0.0",
  "description": "Given an OpenAPI definition, turn into a compiled AJV module.",
  "type": "module",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.min.js",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "uvu src",
    "build": "bundt src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saibotsivad/ajv-openapi-compile.git"
  },
  "keywords": [
    "ajv",
    "openapi",
    "cloudflare"
  ],
  "author": {
    "name": "Tobias Davis",
    "email": "tobias@davistobias.com",
    "url": "https://davistobias.com"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/saibotsivad/ajv-openapi-compile/issues"
  },
  "homepage": "https://github.com/saibotsivad/ajv-openapi-compile",
  "devDependencies": {
    "bundt": "^1.1.5",
    "uvu": "^0.5.2"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1"
  }
}
