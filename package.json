{
  "name": "ajv-openapi-compile",
  "version": "0.0.0",
  "description": "Generate a compiled AJV validation module from an OpenAPI definition.",
  "bin": "bin.js",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "npm run test-js && npm run test-json && npm run test-script",
    "test-json": "./bin.js -d ./test/definition.json -o ./test/compiled.cjs && node test/test.js",
    "test-js": "./bin.js -d ./test/definition.js -o ./test/compiled.cjs && node test/test.js",
    "test-script": "node ./test/pre-test.js && node test/test.js",
    "build": "bundt src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saibotsivad/ajv-openapi-compile.git"
  },
  "keywords": [
    "ajv",
    "openapi",
    "cloudflare"
  ],
  "files": [
    "dist",
    "src",
    "bin.js",
    "README.md",
    "LICENSE.md"
  ],
  "author": {
    "name": "Tobias Davis",
    "email": "tobias@davistobias.com",
    "url": "https://davistobias.com"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/saibotsivad/ajv-openapi-compile/issues"
  },
  "homepage": "https://github.com/saibotsivad/ajv-openapi-compile",
  "devDependencies": {
    "bundt": "github:saibotsivad/bundt",
    "uvu": "^0.5.2"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "mri": "^1.2.0"
  }
}
